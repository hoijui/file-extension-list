#!/usr/bin/env sh
# File extension to category resolver.# Generated by file-extension-list.
# See: https://github.com/dyne/file-extension-list

set -e

ext="$1"
if [ -z "$ext" ] 
then
	>&2 echo "Usage: $0 [FILE-EXT]"
		exit 1
	
fi

case "$ext" in
	7z|a|apk|ar|bz2|cab|cpio|deb|dmg|egg|gz|iso|jar|lha|mar|pak|pea|rar|rpm|s7z|shar|tar|tbz2|tgz|tlz|war|whl|xpi|xz|zip|zipx)
		echo "archive"
		;;
	aac|aiff|ape|au|flac|gsm|it|m3u|m4a|mid|mod|mp3|mpa|pls|ra|s3m|sid|wav|wma|xm)
		echo "audio"
		;;
	azw|azw1|azw3|azw4|azw6|cbr|cbz|epub|mobi)
		echo "book"
		;;
	1.ada|2.ada|ada|adb|ads|asm|bas|c|c++|cbl|cc|class|clj|cob|cpp|cs|cxx|d|diff|e|el|f|f77|f90|for|fth|ftn|go|groovy|h|hh|hpp|hs|htm|html|hxx|java|js|jsp|jsx|kt|lhs|lisp|lua|m|m4|nim|patch|php|pl|po|pp|py|r|rb|rs|s|scala|swg|swift|v|vb|vcxproj|xcodeproj|xml)
		echo "code"
		;;
	bash|bat|bin|command|crx|csh|exe|fish|ksh|msi|sh|zsh)
		echo "exec"
		;;
	eot|otf|ttf|woff|woff2)
		echo "font"
		;;
	3dm|3ds|ai|bmp|dds|dwg|dxf|eps|gif|gpx|jpeg|jpg|kml|kmz|max|png|ps|psd|svg|tga|thm|tif|tiff|webp|xcf)
		echo "image"
		;;
	csv|ics|ods|vcf|xls|xlsx)
		echo "sheet"
		;;
	odp|ppt)
		echo "slide"
		;;
	doc|docx|ebook|log|md|msg|odt|org|pages|pdf|rst|rtf|tex|txt|wpd|wps)
		echo "text"
		;;
	3g2|3gp|aaf|asf|avchd|avi|drc|flv|m2v|m4p|m4v|mkv|mng|mov|mp2|mp4|mpe|mpeg|mpg|mpv|mxf|nsv|ogg|ogm|ogv|qt|rm|rmvb|roq|srt|svi|vob|webm|wmv|yuv)
		echo "video"
		;;
	css|less|scss|wasm)
		echo "web"
		;;
	*)
		>&2 echo "Unknown file extension: $ext"
		exit 1
esac
